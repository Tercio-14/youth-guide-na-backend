{
  "name": "youth-guide-na-backend",
  "version": "1.0.0",
  "description": "RAG-powered backend API for YouthGuide NA - connecting youth to opportunities in Namibia",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "lint": "eslint src/",
    "setup": "node scripts/setup-collections.js",
    "ingest": "node scripts/ingest.js"
  },
  "keywords": [
    "youth",
    "employment",
    "rag",
    "chatbot",
    "namibia",
    "ai"
  ],
  "author": "YouthGuide NA Research Team",
  "license": "MIT",
  "dependencies": {
    "@xenova/transformers": "^2.6.2",
    "axios": "^1.12.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "firebase-admin": "^11.11.1",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "morgan": "^1.10.0",
    "node-fetch": "^3.3.2",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.js"
    ]
  }
}
